<ROSETTASCRIPTS>

    <TASKOPERATIONS>
        <ReadResfile name="resfile"/>
        <ExtraRotamersGeneric name="ex"
            ex1="yes" ex2="yes" extrachi_cutoff="0"/>
    </TASKOPERATIONS>

    <MOVERS>
      <ConstraintSetMover name=cst cst_file="%%cst_file%%"/>
      <PackRotamersMover name="fixbb" task_operations="resfile,ex"/>
    </MOVERS>

    <PROTOCOLS>
        <Add mover_name=cst/>
        <Add mover_name=fixbb/>
    </PROTOCOLS>

</ROSETTASCRIPTS>

