<ROSETTASCRIPTS>

  <INCLUDES file="%%shared_defs%%"/>

  <MOVERS>
    <ConstraintSetMover name="cst" cst_fa_file="%%cst_file%%"/>
    <LoopModeler name="modeler"
      config="kic_with_frags"
      scorefxn="scorefxn_cst"
      task_operations="resfile,ex"
      loops_file="%%loop_file%%"
      fast="%%fast%%">
        <Build skip="yes"/>
    </LoopModeler>
  </MOVERS>

  <PROTOCOLS>
    <Add mover_name=cst/>
    <Add mover_name=modeler/>
  </PROTOCOLS>

</ROSETTASCRIPTS>
